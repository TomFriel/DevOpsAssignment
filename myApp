# myapp.py
import logging
import myLib
logger = logging.getLogger(__name__)

def main():
    logging.basicConfig(filename='myapp.log', level=logging.INFO)
    logger.info('Started')
    myLib.do_something()
    logger.info('Finished')

if __name__ == '__main__':
    main()
    logger.info('Exiting')
    
